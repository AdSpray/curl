{
    "name": "jyggen/curl",
    "description": "A simple and lightweight cURL library with support for multiple requests in parallel.",
    "keywords": ["cURL", "HTTP", "Multi-thread", "Parallel", "Spider", "Bot", "Request", "Crawler"],
    "homepage": "https://github.com/jyggen/curl",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com",
            "homepage": "http://jyggen.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "symfony/http-foundation": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "dev-master@dev",
        "league/phpunit-coverage-listener": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Jyggen\\Curl\\": "src",
            "Jyggen\\Curl\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
